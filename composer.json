    "description": "Neo4j based Recommendation Engine Framework for PHP",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "GraphAware Limited",
            "email": "reco4php@graphaware.com"
        },
        {
          "name": "Darren Craig",
          "email": "darrencraig+github@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "graphaware/neo4j-php-client": "^4.8",
        "symfony/event-dispatcher": "^2.7 || ^3.0 || ^4.0 || ^5.0",
        "psr/log": "^1.1",
        "symfony/stopwatch": "^2.7 || ^3.0 || ^4.0  || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
